{
  "name": "jupiter-wallet",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx vite --port 3002 dev",
    "build": "npx vite build",
    "preview": "npx vite preview",
    "lint": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css)\" --print-width 150",
    "eslint": "eslint --config \".eslintrc.json\" . --fix",
    "eslint_vite": "eslint --config \".eslintrc.json\" --config \".eslint-dev-rules.json\" \"./src\" --fix",
    "test": "react-scripts test",
    "husky_setup": "husky install",
    "coverage": "CI=true react-scripts test --coverage",
    "postcoverage:dark": "cp ./node_modules/nyc-dark/*.css coverage/lcov-report",
    "cypress": "npx cypress open"
  },
  "engines": {
    "npm": ">=8.1.0",
    "node": ">=16.13.1",
    "yarn": ">=1.22.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.1.1",
    "@mui/material": "^5.1.1",
    "@types/react": "^17.0.35",
    "crypto-js": "^4.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "^5.0.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^7.1.2",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@vitejs/plugin-react": "^1.3.0",
    "cypress": "^9.5.3",
    "cypress-dark": "^1.8.3",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "nyc-dark": "^3.0.3",
    "prettier": "^2.4.1",
    "react-is": "^17.0.2",
    "typescript": "^4.5.2",
    "vite": "^2.9.1",
    "vite-plugin-checker": "^0.4.4",
    "vite-plugin-istanbul": "^2.5.1",
    "vite-tsconfig-paths": "^3.4.1"
  }
}
